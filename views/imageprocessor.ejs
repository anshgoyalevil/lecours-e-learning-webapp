<div class="dark:bg-gray-900">

    <%- include("header") %>
    
    <form id="form" action="/upload" method="POST" enctype="multipart/form-data">
    
        <div class="lg:px-60 px-5 lg:pt-20 pt-5">
            
            <label class="block mb-2 text-sm font-medium text-gray-900 dark:text-gray-300" for="user_avatar">Upload the Suspect One (In Image Format)</label>
            <input class="block w-full text-sm text-gray-900 bg-gray-50 rounded-lg border border-gray-300 cursor-pointer dark:text-gray-400 focus:outline-none dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400" aria-describedby="user_avatar_help" id="file1" type="file" name="file1" required>
            
            
        </div>
        
        <div class="lg:px-60 px-5 lg:pt-10 pt-5 pb-20">
            
            <label class="block mb-2 text-sm font-medium text-gray-900 dark:text-gray-300" for="user_avatar">Upload the Suspect Two (In Image Format)</label>
            <input class="block w-full text-sm text-gray-900 bg-gray-50 rounded-lg border border-gray-300 cursor-pointer dark:text-gray-400 focus:outline-none dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400" aria-describedby="user_avatar_help" id="file2" type="file" name="file2" required>
            <button type="submit" class="text-white bg-blue-700 hover:bg-blue-800 focus:ring-4 focus:ring-blue-300 font-medium rounded-lg text-sm px-5 py-2.5 dark:bg-blue-600 dark:hover:bg-blue-700 focus:outline-none dark:focus:ring-blue-800 mt-10 mb-5 mr-5">Check Now</button>
            
                
        </div>
        
    </form>
        <%- include("footer") %>
        
    </div>