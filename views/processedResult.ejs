<%- include("header") %>

<section class="bg-white dark:bg-gray-900">
    <div class="max-w-screen-xl px-4 py-8 mx-auto text-center lg:py-16 lg:px-6">
        <figure class="max-w-screen-md mx-auto">
            <svg class="h-12 mx-auto mb-3 text-gray-400 dark:text-gray-600" viewBox="0 0 24 27" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M14.017 18L14.017 10.609C14.017 4.905 17.748 1.039 23 0L23.995 2.151C21.563 3.068 20 5.789 20 8H24V18H14.017ZM0 18V10.609C0 4.905 3.748 1.038 9 0L9.996 2.151C7.563 3.068 6 5.789 6 8H9.983L9.983 18L0 18Z" fill="currentColor"/>
            </svg> 
            <blockquote>
                <p class="text-2xl font-medium text-gray-900 dark:text-white">Your Image Similarity Score is:</p>
            </blockquote>
            <figcaption class="items-center justify-center mt-6 space-x-3">
                <div class="divide-gray-500 dark:divide-gray-700">
                    <div class="pr-3 text-2xl font-extrabold text-gray-900 dark:text-white"><%= score %> </div>
                </div>
                <br>
                <div class="divide-gray-500 dark:divide-gray-700">
                    <div class="pr-3 font-medium text-gray-900 dark:text-white">0 Score Means Images are Similar & High score means images are very unique</div>
                </div>
            </figcaption>
        </figure>
        <a href="/image-processor">

            <button type="button" class="text-white bg-blue-700 hover:bg-blue-800 focus:ring-4 focus:ring-blue-300 font-medium rounded-lg text-sm px-5 py-2.5 dark:bg-blue-600 dark:hover:bg-blue-700 focus:outline-none dark:focus:ring-blue-800 mt-10 mb-5 mr-5" id="showbtn">Check Again</button>
        </a>
    </div>
  </section>

  <script src="../index.js"></script>
<%- include("footer") %>